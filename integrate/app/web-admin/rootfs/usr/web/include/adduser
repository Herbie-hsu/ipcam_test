function focusOnFirst()
{
  var form = document.useraddForm;
  form.user.focus()
  form.user.select()
}

function checkUserName()
{
  var form = document.useraddForm;
  var aName = form.user.value;
  var c;
 // var currentUsers=[];
   var userList = window.opener.document.forms[0].elements[0];
  // Check empty username
  if (aName.charAt(0) == '') {
    alert(User_name_too_short)
    form.user.focus()
    form.user.select()
    return false
  }
	
  // Check for too long username
 
  if (aName.length > 14) {
    alert(User_name_too_long)
    form.user.focus()
    form.user.select()
    return false
  }

  // Check invalid characters
  c = aName.charAt(0)
  if (!((c>='A' && c<='Z') || (c>='a' && c<='z'))) {
    alert(User_name_first_char_invalid)
    form.user.focus()
    form.user.select()
    return false
  }

  for (var i = 0; i < aName.length; i++) {
    c = aName.charAt(i)
    if (!((c>='A' && c<='Z') || (c>='a' && c<='z') || (c>='0' && c<='9') || (c =='_'))) {
      alert(User_name_invalid)
      form.user.focus()
      form.user.select()
      return false
    }
  }

